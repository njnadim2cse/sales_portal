<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for LC Order -->
        <record id="seq_lc_order" model="ir.sequence">
            <field name="name">LC Order Sequence</field>
            <field name="code">lc.order</field>
            <field name="prefix">LCO-</field>
