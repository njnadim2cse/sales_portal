
<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_report_form_docx" model="ir.ui.view">
        <field name="name">ir.actions.report.form.docx</field>
        <field name="model">ir.actions.report</field>
        <field name="inherit_id" ref="web.report_form_view"/>
        <field name="arch" type="xml">
             <xpath expr="//field[@name='report_type']" position="after">
                <field name="is_docx_report"/>
                <field name="docx_template" filename="docx_filename"/>
                <field name="docx_filename" invisible="1"/>
            </xpath>
        </field>
    </record>
</odoo>

  